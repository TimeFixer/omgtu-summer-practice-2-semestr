// Для BlockingCollection
#r "nuget:ScottPlot, 5.0.36"
using Microsoft.DotNet.Interactive.Formatting;
using System.Collections.Concurrent;
using System.Threading;
using System.Diagnostics;

long[] ReadWrite = new long[10];
long[] Read = new long[10];

for (int i = 0; i<10; i++){
BlockingCollection<object> blockcollect =  new BlockingCollection<object>();
ManualResetEvent ResentStart = new ManualResetEvent(false);
ManualResetEvent ResentComplete= new ManualResetEvent(false);
long countObject = 1000000;
bool flag = true;
Thread threadW = new Thread(() =>
{
    ResentStart.WaitOne();
    while(flag)
        {
        if(countObject>0)
        {
                blockcollect.Add(new object());
                Interlocked.Decrement(ref countObject);
        }
        if(countObject == 0 && blockcollect.Count==0)
            flag = false;
    }
});
Thread threadR = new Thread(() =>
{
    ResentStart.WaitOne();
    Stopwatch WatchRead = Stopwatch.StartNew();
    while(flag)   
        {
            if(blockcollect.Count != 0)
            {
                blockcollect.Take();
            }
        }
    ResentComplete.Set();
    WatchRead.Stop();
    Read[i] = WatchRead.ElapsedMilliseconds;
});
threadW.Start();
threadR.Start();

ResentStart.Set();

Stopwatch TimeReadWrite = Stopwatch.StartNew();
ResentComplete.WaitOne();
TimeReadWrite.Stop();
threadW.Join();
threadR.Join();
ReadWrite[i] = TimeReadWrite.ElapsedMilliseconds;
}
var ReadavverageFORBlockingCollection = Read.Average();
var ReadWriteavverageFORBlockingCollection = ReadWrite.Average();

