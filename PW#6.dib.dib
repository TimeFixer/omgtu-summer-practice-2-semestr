ScottPlot.Plot graph1 = new();
ScottPlot.Plot graph2 = new();
graph1.Add.Scatter(numbers, Read);
graph2.Add.Scatter(numbers, ReadWrite);
Console.WriteLine("Время чтения");
graph1

